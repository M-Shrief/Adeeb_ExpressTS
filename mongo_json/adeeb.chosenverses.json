  },
  "updatedAt": {
    "$date": "2022-12-16T14:22:23.392Z"
  },
  "__v": 0,
  "verses": [
    {
      "first": "نَظْرةٌ, ثُمَّ هَوًى, ثُمَّ مُنًى",
      "sec": "ثُمَّ .. وَانْفَضَ كَأَنْ لم تَحْلُمِ",
      "_id": {
        "$oid": "639c7f1fb95190b2fdf1546a"
      }
    },
    {
      "first": "لا أرى إلا فَنَاءً أو سُدًى",
      "sec": "فَبَصِيرٌ في ضَلالٍ أو عَمِ",
      "_id": {
        "$oid": "639c7f1fb95190b2fdf1546b"
      }
    }
  ]
},
{
  "_id": {
    "$oid": "639c7f5eb95190b2fdf1546d"
  },
  "poet": {
    "$oid": "639b5cf712eec0bb274cecd4"
