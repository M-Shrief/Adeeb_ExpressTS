    "$date": "2022-12-16T14:20:46.236Z"
  },
  "updatedAt": {
    "$date": "2022-12-16T14:20:46.236Z"
  },
  "__v": 0,
  "verses": [
    {
      "first": "زهرةٌ حَنَّتْ, فباحت؛ فذوت",
      "sec": "أذْبَلَتها نَفْحةٌ لم تُكْتَمِ",
      "_id": {
        "$oid": "639c7ebeb95190b2fdf15466"
      }
    },
    {
      "first": "شكتِ البِثَّ لِنجمٍ ساطعٍ",
      "sec": "ثُمَّ ظَلَّتْ في شُعاعٍ مُلهِمِ",
      "_id": {
        "$oid": "639c7ebeb95190b2fdf15467"
      }
    }
  ]
},
{
  "_id": {
    "$oid": "639c7f1fb95190b2fdf15469"
  },
  "poet": {
    "$oid": "639b5cf712eec0bb274cecd4"
  },
  "poem": {
    "$oid": "639c7bf9b95190b2fdf1527a"
  },
  "tags": "حزن,نفس,القدر",
