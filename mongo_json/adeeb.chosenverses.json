  },
  "updatedAt": {
    "$date": "2022-12-16T14:19:37.358Z"
  },
  "__v": 0,
  "verses": [
    {
      "first": "فهوَ أمواجُ ظلامٍ .. لا تَرَى",
      "sec": "لا تُبَالي .. لا تَعِي .. لا تَحْتَمي",
      "_id": {
        "$oid": "639c7e79b95190b2fdf15463"
      }
    }
  ]
},
{
  "_id": {
    "$oid": "639c7ebeb95190b2fdf15465"
