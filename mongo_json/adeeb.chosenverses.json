  },
  "__v": 0,
  "verses": [
    {
      "first": "وَ لَيَالٍ أظْلَمَت أنوَارُهَا",
      "sec": "وَ لَيَالٍ نُورُهَا لم يُظْلِمِ",
      "_id": {
        "$oid": "639c7f5eb95190b2fdf1546e"
      }
    }
  ]
},
