  "updatedAt": {
    "$date": "2022-12-16T14:14:44.864Z"
  },
  "__v": 0,
  "verses": [
    {
      "first": "حسرةٌ ولَّت, و أخرى أقبلت",
      "sec": "كيفَ؟ مِن أينَ؟ .. متى؟ لم أعلمِ",
      "_id": {
        "$oid": "639c7d54b95190b2fdf15458"
      }
    },
    {
      "first": "موجةٌ سوداءُ تنقضُّ على",
      "sec": "موجةٍ في بحر ليلٍ مظلمِ",
      "_id": {
        "$oid": "639c7d54b95190b2fdf15459"
      }
    }
  ]
},
{
  "_id": {
    "$oid": "639c7e79b95190b2fdf15462"
  },
  "poet": {
    "$oid": "639b5cf712eec0bb274cecd4"
